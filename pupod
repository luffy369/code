#include <iostream>
#include <stack>

using namespace std;

int main() {
  stack<int> s;
    s.push(10);
    s.push(20);
    s.push(30);

  cout << "Top element of the stack: " << s.top() << endl;

  s.pop();
  cout << "Top element of the stack: " << s.top() << endl;

  cout << "Elements of the stack: ";
  while (!s.empty()) {
    cout << s.top() << " ";
    s.pop();
  }
  cout << endl;

  return 0;
}
